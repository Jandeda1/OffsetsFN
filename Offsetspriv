#define TinyFont 0x30 // UEngine::TinyFont

// ===========================================
// LOCAL PLAYER OFFSETS - REAL FORTNITE
// ===========================================
#define ClientSettingsRecord 0x378 // UFortLocalPlayer::ClientSettingsRecord
#define ViewportClient 0x78 // ULocalPlayer::ViewportClient

// ===========================================
// LEVEL OFFSETS - REAL FORTNITE
// ===========================================
#define OwningWorld 0xC0 // ULevel::OwningWorld
#define WorldSettings 0x2C0 // ULevel::WorldSettings

// ===========================================
// WORLD OFFSETS - REAL FORTNITE
// ===========================================
#define Levels 0x1F0 // UWorld::Levels
#define OwningGameInstance 0x250 // UWorld::OwningGameInstance
#define PersistentLevel 0x40 // UWorld::PersistentLevel

// ===========================================
// GAME STATE BASE OFFSETS - REAL FORTNITE
// ===========================================
#define ServerWorldTimeSecondsDelta 0x2E0 // AGameStateBase::ServerWorldTimeSecondsDelta

// ===========================================
// ACTOR OFFSETS - REAL FORTNITE
// ===========================================
#define CustomTimeDilation 0x68 // AActor::CustomTimeDilation

// ===========================================
// CHARACTER MOVEMENT OFFSETS - REAL FORTNITE
// ===========================================
#define LastUpdateLocation 0x360 // UCharacterMovementComponent::LastUpdateLocation

// ===========================================
// CAMERA CACHE OFFSETS - REAL FORTNITE
// ===========================================
#define CameraCachePrivate 0x1530 // APlayerCameraManager::CameraCachePrivate
#define POV 0x10 // FCameraCacheEntry::POV

// ===========================================
// MINIMAL VIEW INFO OFFSETS - REAL FORTNITE
// ===========================================
#define Rotation 0x18 // FMinimalViewInfo::Rotation

// ===========================================
// PLAYER CONTROLLER ADDITIONAL OFFSETS - REAL FORTNITE
// ===========================================
#define SmoothTargetViewRotationSpeed 0x3A8 // APlayerController::SmoothTargetViewRotationSpeed

// ===========================================
// PLAYER STATE ADDITIONAL OFFSETS - REAL FORTNITE
// ===========================================
#define CompressedPing 0x2B0 // APlayerState::CompressedPing
#define PawnPrivate 0x320 // APlayerState::PawnPrivate
#define PlayerId 0x2AC // APlayerState::PlayerId
#define PlayerNamePrivate 0x340 // APlayerState::PlayerNamePrivate

// ===========================================
// WORLD SETTINGS OFFSETS - REAL FORTNITE
// ===========================================
#define WorldGravityZ 0x328 // AWorldSettings::WorldGravityZ

// ===========================================
// CALCULATED OFFSETS - REAL FORTNITE
// ===========================================
#define Bounds 0x108 // USceneComponent::Bounds
#define LastRenderTime 0x1A0 // UWorld::LastRenderTime
#define LastRenderTimeOnScreen 0x32C // UPrimitiveComponent::LastRenderTimeOnScreen
#define LocalPlayerCachedLODDistanceFactor 0x3AC // APlayerController::LocalPlayerCachedLODDistanceFactor

// ===========================================
// BITFIELD INDEXES - REAL FORTNITE
// ===========================================
#define WireframeBit 7
#define AbsoluteLocationBit 2
#define ActiveBit 2
#define AllowTargetingBit 4
#define AllowTargetingDuringReloadBit 0
#define AlreadySearchedBit 2
#define AutoRunOnBit 3
#define ComponentToWorldUpdatedBit 0
#define ForceWireframeBit 6
#define IsABotBit 3
#define IsCreativeModeratorModeEnabledBit 5
#define IsDBNOBit 7
#define IsDyingBit 5
#define IsInvulnerableBit 2
#define IsReloadingWeaponBit 0
#define ThankedBusDriverBit 0

// ===========================================
// ARRAY OFFSETS - REAL FORTNITE
// ===========================================
#define AActors 0x1A8 // AActors (Dumped With UDMANS Offset Dumper)
#define LevelsArray 0x1E8 // Levels array
#define ActorsArray 0x48 // Actors array

// ===========================================
// GLOBAL OFFSETS - REAL FORTNITE
// ===========================================
#define GEngine 0x17B93638 // GEngine (Dumped With UDMANS Offset Dumper)
#define GNames 0x17A31B80 // GNames global

